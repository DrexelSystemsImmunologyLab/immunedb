#!/usr/bin/env python2
import argparse
import sys
from sldb.trees.newick2json import run_newick2json

if __name__ == '__main__':
    parser = argparse.ArgumentParser(description='Newick to JSON')
    parser.add_argument('tab_file')
    parser.add_argument('newick_file')
 
    sys.exit(run_newick2json(parser.parse_args()))
